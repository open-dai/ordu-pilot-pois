<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sampas.ortak.spatial.generator.model">
	<class name="GeneratedID" table="GLOB_GENERATED_ID">
		
		<id name="id" column="ID">
			<generator class="native">
				<param name="sequence">SEQ_GLOB_GENERATED_ID</param>
			</generator>
		</id>
		
		<property name="className" type="java.lang.String">
			<column name="CLASS_NAME" />
		</property>
		
		<property name="lastGenerated" type="java.lang.Long">
			<column name="LAST_GENERATED_ID" />
		</property>
		
	</class>
</hibernate-mapping>